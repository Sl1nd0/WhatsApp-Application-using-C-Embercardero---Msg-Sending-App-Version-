#include "clsWH.h"

TbackWhat:: TbackWhat()
{
	name = " ";
	number = " ";
	gender = " ";
	prov = " ";
}
//---------------------------------------------------------------------------

void TbackWhat::setData(AnsiString nm, AnsiString num, AnsiString gen, AnsiString prv)
{
   name = nm;
   number = num;
   gender = gen;
   prov = prv;
}
//---------------------------------------------------------------------------

AnsiString TbackWhat::getName()
{
	return name;
}
//---------------------------------------------------------------------------

AnsiString TbackWhat::getNumber()
{
   return name;
}
//---------------------------------------------------------------------------

AnsiString TbackWhat::getGender()
{
	return name;
}
//---------------------------------------------------------------------------

AnsiString TbackWhat::getProv()
{
    return name;
}
//---------------------------------------------------------------------------

